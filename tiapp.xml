<!--Replace the ios, android, modules, and sdk-version tags in your tiapp.xml with the following-->    
    
    <!--REPLACE THE ios TAG WITH THE FOLLOWING-->
    <ios>
        <plist>
            <dict>
                <key>UISupportedInterfaceOrientations~iphone</key>
                <array>
                    <string>UIInterfaceOrientationPortrait</string>
                </array>
                <key>UISupportedInterfaceOrientations~ipad</key>
                <array>
                    <string>UIInterfaceOrientationPortrait</string>
                    <string>UIInterfaceOrientationPortraitUpsideDown</string>
                    <string>UIInterfaceOrientationLandscapeLeft</string>
                    <string>UIInterfaceOrientationLandscapeRight</string>
                </array>
                <key>UIRequiresPersistentWiFi</key>
                <false/>
                <key>UIPrerenderedIcon</key>
                <false/>
                <key>UIStatusBarHidden</key>
                <false/>
                <key>UIStatusBarStyle</key>
                <string>UIStatusBarStyleDefault</string>
                <key>NSLocationWhenInUseUsageDescription</key>
                <string>
                    Location is used to determine the distance between your location and a service provider.
                </string>
            </dict>
        </plist>
    </ios>
    
    <!--REPLACE THE android TAG WITH THE FOLLOWING-->
    <android xmlns:android="http://schemas.android.com/apk/res/android">
        <manifest>
            <!-- Allows the API to download data from Google Map servers -->
            <uses-permission android:name="android.permission.INTERNET"/>
            <!-- Allows the API to cache data -->
            <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
            <!-- Use GPS for device location -->
            <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
            <!-- Use Wi-Fi or mobile connection for device location -->
            <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
            <!-- Allows the API to access Google web-based services -->
            <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
            <!-- Specify OpenGL ES 2.0 as a requirement -->
            <uses-feature android:glEsVersion="0x00020000" android:required="true"/>
            <!-- Replace <com.domain.appid> with your application ID -->
            <uses-permission android:name="org.fsadayton.armorapp.permission.MAPS_RECEIVE"/>
            <permission
                android:name="org.fsadayton.armorapp.permission.MAPS_RECEIVE" android:protectionLevel="signature"/>
            <application android:theme="@style/Theme.CustomActionBar">
                <!-- Replace "PASTE YOUR GOOGLE MAPS API KEY HERE" with the Google API key you obtained -->
                <meta-data
                    android:name="com.google.android.maps.v2.API_KEY" android:value="AIzaSyDCmJ4lBpceIqcWtJI9QBws_-l9W6Q5arg"/>
            </application>
            <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="21"/>
        </manifest>
    </android>
    
    <!--REPLACE THE modules TAG WITH THE FOLLOWING-->
    <modules>
        <module platform="commonjs">ti.cloud</module>
        <module platform="iphone" version="2.4.1">ti.map</module>
        <module platform="android" version="2.3.3">ti.map</module>
        <module platform="android" version="1.1.0">ti.geocoder</module>
        <module platform="iphone" version="1.0.0">ti.geocoder</module>
        <module platform="android" version="1.6.7">com.alcoapps.actionbarextras</module>
        <module platform="android" version="1.0.0">ti.android.sms</module>
        <module platform="iphone" version="1.1.2">com.omorandi</module>
        <module platform="iphone" version="1.8.2">dk.napp.social</module>
    </modules>
    
    <!--REPLACE THE sdk-version TAG WITH THE FOLLOWING-->
    <sdk-version>5.0.2.GA</sdk-version>