<Alloy>
	<Window id="win" class="container">
		<Require src="header"></Require>
		<View id="accountInfo"></View>
		<View>
			<ImageView id="accountImage" image="/global/accountImage.png" />
			<Label id="firstNameLine">First Name</Label>
			<Label id="firstName">Sam</Label>
			<Label id="lastNameLine">Last Name</Label>
			<Label id="lastName">Sparks</Label>
			<Label id="dobLine">DOB</Label>
			<Label id="dob">3/17/90</Label>
			<Label id="addressLine">Current Address</Label>
			<Label id="address">1793 S. 4th St. SanfranSokyo CA, 45789</Label>
		</View>
		
		<TableView id="contactTable">
			<TableViewRow>
				<View>
					<Label id="trustedContacts">Trusted Contacts</Label>
				</View>
			</TableViewRow>
			<TableViewRow>
				<View>
					<Label id="name">Name</Label>
					<Label id="phone">Phone</Label>
					<Label id="email">Email</Label>
				</View>
			</TableViewRow>
		</TableView>	
	</Window>
</Alloy>