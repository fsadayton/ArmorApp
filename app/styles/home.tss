".container": {
	backgroundColor:"#fff",
}
".boxContainer":{
	height:"28%",
	color:"#7d7d7d"
}

"#welcomeLabel[platform=android]":{
	color:"#143963",
	bottom:"5dp",
	top:"7dp",
	textAlign:"center",
	height: "10%"
}

"#welcomeLabel[platform=ios]":{
	color:"#143963",
	bottom:"5dp",
	top:"9dp",
	textAlign:"center",
	height: "80px",
	width: "381px"
}

".box":{
	borderRadius: 10,
	borderWidth: 2,
	borderColor: "#143963",
	height: "90%",
	width: "45%",
	layout:"vertical"
}

".left":{
	left:"10dp"
}

".right":{
	right:"10dp"
}

".name":{
	font:{
		fontFamily:"Dosis-Bold",
		fontSize:'14dp'
	},
	top:"1dp",
	color:"#143963",
	height:"30%",
	textAlign:"center"
}
".icon":{
	height: "25%",
	top:"1dp",
	bottom:"1dp"
}

".description":{
	bottom:"5dp",
	width:"95%",
	font:{
		fontSize:'10dp'
	},
	textAlign: "center",
	height:"30%",
	color:"#143963"
}
"#header": {
	backgroundColor: "#00a1ff",
	opacity: 1,
	color: "#143963",
	height: "8%",
	top: 0
}

"#headerText":{
	color: "#ffd400",
	font:{
		fontSize: '24dp',
		fontWeight: 'bold'
	}
}

"#headerLeft":{
	left: "6%",
	width: "11%"
}

"#headerRight":{
	right: "6.5%",
	width: "11%"
}



