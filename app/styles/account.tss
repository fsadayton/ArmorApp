"Window": {
	backgroundColor: "#fff",
	layout: "vertical"
}

"#accountImage":{
	height:"45dp",
	width:"45dp",
	borderRadius: 20
}

"#name":{
	font:{
		fontFamily:"Dosis-Bold",
		fontSize:"25dp"
	},
	color:"#009577",
	textAlign: "center",
	left:"5dp"
}

".headerLabel":{
	color:"#009577",
	height:"15dp",
	top:"10dp"
}

"#contactTableView":{
	height:"45%"
}

"TableViewRow":{
	height:"45dp",
	color:"#666"
}

"#profileBar":{
	width: "70%",
	height:"42dp",
	borderRadius: 20,
	borderColor: "#009577",
	borderWidth: 2,
	layout: "horizontal",
	top: "5dp"
}

".profileButton":{
	height:"30dp",
	width:"30dp",
	zIndex:0,
	touchEnabled: false
}

".profileView":{
	width: "33.3%",
	backgroundColor: "#fff",
	zIndex: 1,
	borderColor: "#009577",
	borderWidth: 1
}

"TextField":{
	font:{
		fontFamily:"Dosis-Bold"
	},
	color:"#7d7d7d"
}

".sentence":{
	left:"1dp",
	right:"1dp",
	font:{
		fontSize:"20dp"
	},
	width:Titanium.UI.SIZE,
	horizontalWrap:true
}

"#trustedMessageEdit":{
	width:"90%",
	borderRadius:15,
	borderColor: "#666",
	font:{
		fontSize:"15dp",
		fontFamily:"Dosis-Regular"
	},
	height:"20%",
	top:"5dp"
}





