	top:"15dp"
}
"#address":{
	left:"10dp",
	width:"50%",
	color:"#143963"
}
"#addressView":{
	height:"55dp"
}

"ImageView":{
	width:"30dp"
}

".actionView":{
	height:"45dp",
	width:"90%",
	borderRadius:20,
	borderWidth: 2,
	borderColor:"#f9c84d",
