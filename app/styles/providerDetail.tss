
"Window":{
	backgroundColor:"#fff",
	layout:"vertical"
}

"#providerName":{
	backgroundColor:"#D5D5D5",
	font:{
		fontFamily:"Dosis-Bold",
		fontSize: "20dp"
	},
	textAlign: "center",
	color:"#fff",
	width:"100%"
}

"#providerDescription":{
	borderColor:"#009577",
	borderRadius: 10,
	borderWidth: 2,
	color:"#666",
	editable: false,
	focusable: false,
	height:"20%",
	font:{
		fontFamily:"Dosis-Regular",
		fontSize: "15dp"
	},
	top:"10dp",
	width: "90%"
}
"#addressLabel":{
	font:{
		fontFamily:"Dosis-Bold",
		fontSize: "15dp"
	},
	color:"#009577",
